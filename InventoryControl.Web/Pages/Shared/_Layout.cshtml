<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Inventory Control</title>
    <!-- CSS -->
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet"
        integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous">
    <link rel="stylesheet" href="~/styles.css">
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.bundle.min.js"
        integrity="sha384-ka7Sk0Gln4gmtz2MlQnikT1wXgYsOg+OMhuP+IlRH9sENBO0LRn5q+8nbTov4+1p"
        crossorigin="anonymous"></script>
    <script src="~/script.js" defer></script>
</head>

<body>
    <header>
        <p>Inventory Control</p>
        <div class="Toolbar">
            @if (TempData.ContainsKey("UserType"))
            {
                var userType = TempData["UserType"] as int?;
                <nav>
                    <ul>
                        @if (userType == 1)
                        {
                            <a href="#solicitudesActualesContent">Inicio</a>
                            <a href="#pedidosActualesContent">Pedidos</a>
                            <a href="#hacerPedidoContent">Solicitud</a>
                            <a href="#historialSolicitudesContent">Historial Docente</a>
                            <a href="#historialSolicitudesEstContent">Historial Alumnos</a>
                            <a href="#materialesContent">Materiales</a>
                            <a href="/index">Log Out</a>
                            <a href="javascript:void(0);" class="icon" onclick="myFunction()">
                                &#9776;
                            </a>
                        }
                        else if (userType == 2)
                        {
                            <a href="#pedidosActualesContent">Inicio</a>
                            <a href="#hacerPedidoContent">Pedido</a>
                            <a href="#historialContent">Historial</a>
                            <a href="/index">Log Out</a>
                            <a href="javascript:void(0);" class="icon" onclick="myFunction()">
                                &#9776;
                            </a>
                        }
                        else if (userType == 3)
                        {
                            <a href="#SApDContent">Inicio</a>
                            <a href="#SApEContent">Solicitudes Estudiantes</a>
                            <a href="#SNApEContent">Solicitudes No Aprob.</a>
                            <a href="#HTSContent">Historial</a>
                            <a href="#cContent">Categorias</a>
                            <a href="#marcaContent">Marca</a>
                            <a href="#modeloContent">Modelo</a>
                            <a href="#materialContent">Material</a>
                            <a href="#mantContent">Mantenimiento</a>
                            <a href="#reportmantContent">Reportes de Mant.</a>
                            <a href="#MPContent">Entregas</a>
                            <a href="/index">Log Out</a>
                            <a href="javascript:void(0);" class="icon" onclick="myFunction()">
                                &#9776;
                            </a>
                        }
                        else if (userType == 4)
                        {
                            <a href="#SApDContent">Inicio</a>
                            <a href="#SApEContent">Solicitudes Estudiantes</a>
                            <a href="#SNApEContent">Solicitudes No Aprob.</a>
                            <a href="#HTSContent">Historial</a>
                            <a href="#hacerPedidoContent">Pedido</a>
                            <a href="#cContent">Categorias</a>
                            <a href="#marcaContent">Marca</a>
                            <a href="#modeloContent">Modelo</a>
                            <a href="#materialContent">Material</a>
                            <a href="#mantContent">Mantenimiento</a>
                            <a href="#reportmantContent">Reportes Mant</a>
                            <a href="#grupoContent">Grupos</a>
                            <a href="#MPContent">Materiales Prestados</a>
                            <a href="#docenteContent">Docentes</a>
                            <a href="#alumnoContent">Alumnos</a>
                            <a href="#almacenistaContent">Almacenistas</a>
                            <a href="#MPContent">Entregas</a>
                            <a href="/index">Log Out</a>
                            <a href="javascript:void(0);" class="icon" onclick="myFunction()">
                                &#9776;
                            </a>
                        }
                    </ul>
                </nav>
            }
        </div>
        
    </header>
    <div class="container">
        @RenderBody()
    </div>
    <hr />
    <footer>
        <p>&copy; 2023 @ViewData["Title"]</p>
    </footer>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.bundle.min.js"
        integrity="sha384-ka7Sk0Gln4gmtz2MlQnikT1wXgYsOg+OMhuP+IlRH9sENBO0LRn5q+8nbTov4+1p"
        crossorigin="anonymous"></script>
    @RenderSection("Scripts", required: false)
</body>

</html>